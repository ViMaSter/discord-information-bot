{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Bot",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\src\\index.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "env": {
        "DISCORD_API_VERSION": "",
        "DISCORD_TOKEN": "",
        "CLIENT": "",
        "GUILD": "",
      }
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Schedule-Script",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\src\\tasks\\schedule.js",
      "cwd": "${workspaceFolder}",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "env": {
        "WEBHOOK_ID": "",
        "WEBHOOK_TOKEN": ""
      }
    }
  ]
}